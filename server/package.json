{
  "name": "victim-server",
  "version": "0.0.1",
  "private": true,
  "homepage": "/",
  "scripts": {
    "build": "react-scripts build; rm -rf dist/* && babel src --ignore __tests__ --ignore __mocks__ --out-dir dist",
    "start": "npm run migrate; node dist/app.js",
    "develop": "export NODE_ENV=development; babel-node ./src/app.js",
    "watch": "nodemon --exec npm run develop",
    "test": "eslint ./src; jest --coverage",
    "eslint": "eslint ./src",
    "migrate": "knex migrate:latest",
    "web": "react-scripts start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/src/help/jest-setup.js"
  },
  "dependencies": {
    "argon2": "^0.14.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-native": "^1.9.0",
    "bookshelf": "^0.10.2",
    "bookshelf-archive": "^1.0.3",
    "bookshelf-paranoia": "^0.10.4",
    "chalk": "^1.1.3",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.14.0",
    "express-graphql": "^0.6.3",
    "faker": "^3.1.0",
    "graphql": "^0.9.1",
    "graphql-tower": "^1.4.2",
    "iconv-lite": "^0.4.13",
    "jest": "^16.0.1",
    "jest-expect-queried": "^1.1.0",
    "jest-matcher-utils": "^16.0.0",
    "jsonwebtoken": "^7.3.0",
    "knex": "^0.12.5",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.6",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.10.2",
    "pg": "^6.1.2",
    "primer-css": "^6.0.0",
    "query-string": "^4.2.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-scripts": "^1.0.7",
    "redis": "2.6.2",
    "styled-components": "^2.0.0",
    "taiwanid": "^1.3.0"
  }
}
